{
  "name": "rollup-plugin-export-equals",
  "version": "0.2.4",
  "description": "Transforms `export default something` to `export = something` for CommonJS module export",
  "main": "dist/export-equals.cjs.js",
  "module": "dist/export-equals.es.js",
  "bundlib": {
    "equals": true
  },
  "typings": "types/index.d.ts",
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "export",
    "exports",
    "equals"
  ],
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "clean": "rm -rf .cache dist types coverage",
    "dev": "bundlib -dw",
    "build": "npm run lint && bundlib -d",
    "test": "jest --colors",
    "preversion": "npm run clean && npm run build && npm test"
  },
  "files": [
    "dist",
    "types",
    "src"
  ],
  "peerDependencies": {
    "rollup": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.10",
    "bundlib": "^0.5.2",
    "eslint": "^6.0.0",
    "jest": "^24.8.0",
    "rollup-plugin-dts": "0.15.1",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "author": "Manuel Fernandez <manferlo81@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/rollup-plugin-export-equals.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/rollup-plugin-export-equals/issues"
  },
  "homepage": "https://github.com/manferlo81/rollup-plugin-export-equals#readme",
  "license": "MIT"
}
