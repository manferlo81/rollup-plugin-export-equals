{
  "name": "rollup-plugin-export-equals",
  "version": "0.3.5",
  "description": "Transforms export default something to export = something for CommonJS module export",
  "main": "dist/export-equals.js",
  "module": "dist/export-equals.mjs",
  "bundlib": {
    "esModule": true,
    "interop": true,
    "equals": true
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "export",
    "exports",
    "equals"
  ],
  "scripts": {
    "lint": "eslint src/** __test__/**",
    "clean": "rimraf node_modules/.cache/bundlib node_modules/.cache/jest node_modules/.cache/.temp dist types coverage",
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build",
    "release": "standard-version"
  },
  "files": [
    "dist",
    "types"
  ],
  "peerDependencies": {
    "rollup": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "bundlib": "^0.13.4",
    "chokidar": "^3.3.0",
    "eslint": "^6.7.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "rollup-plugin-dts": "^1.1.12",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.7.2"
  },
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/rollup-plugin-export-equals.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/rollup-plugin-export-equals/issues"
  },
  "homepage": "https://github.com/manferlo81/rollup-plugin-export-equals#readme",
  "license": "MIT"
}
