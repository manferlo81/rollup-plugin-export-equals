{
  "name": "rollup-plugin-export-equals",
  "version": "0.3.6",
  "description": "Transforms export default something to export = something for CommonJS module export",
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "export",
    "exports",
    "equals"
  ],
  "main": "dist/export-equals.js",
  "module": "dist/export-equals.mjs",
  "bundlib": {
    "esModule": true,
    "interop": true
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "scripts": {
    "lint": "eslint src/** __test__/**",
    "clean": "rimraf node_modules/.cache/bundlib node_modules/.cache/jest node_modules/.cache/.temp dist types coverage",
    "dev": "bundlib -dw",
    "build": "bundlib -d",
    "test": "jest --color -w 3",
    "prerelease": "npm run clean && npm run lint && npm test && npm run build",
    "release": "standard-version"
  },
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.3",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "bundlib": "^0.16.0",
    "chokidar": "^3.3.1",
    "eslint": "^6.8.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.7.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-dts": "^1.4.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-export-equals": "^0.3.6",
    "rollup-plugin-typescript2": "^0.27.0",
    "standard-version": "^7.1.0",
    "ts-jest": "^25.4.0",
    "typescript": "^3.8.3"
  },
  "peerDependencies": {
    "rollup": "^2.0.0"
  },
  "files": [
    "dist",
    "types"
  ],
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/rollup-plugin-export-equals.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/rollup-plugin-export-equals/issues"
  },
  "homepage": "https://github.com/manferlo81/rollup-plugin-export-equals#readme",
  "license": "MIT"
}
