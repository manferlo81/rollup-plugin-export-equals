{
  "name": "rollup-plugin-export-equals",
  "version": "0.3.3",
  "description": "Transforms export default something to export = something for CommonJS module export",
  "main": "dist/export-equals.cjs.js",
  "module": "dist/export-equals.es.js",
  "bundlib": {
    "esModule": true,
    "interop": true,
    "equals": true
  },
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "keywords": [
    "rollup",
    "plugin",
    "rollup-plugin",
    "export",
    "exports",
    "equals"
  ],
  "scripts": {
    "lint": "tslint src/**/*.ts",
    "clean": "rm -rf node_modules/.cache/bundlib node_modules/.cache/jest node_modules/.cache/.temp dist types coverage",
    "dev": "bundlib -dw",
    "build": "npm run lint && bundlib -d",
    "test": "jest --color",
    "preversion": "npm run clean && npm test && npm run build"
  },
  "files": [
    "dist",
    "types"
  ],
  "peerDependencies": {
    "rollup": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "bundlib": "^0.12.0",
    "chokidar": "^3.0.2",
    "eslint": "^6.1.0",
    "husky": "^3.0.1",
    "jest": "^24.8.0",
    "rollup-plugin-dts": "^1.1.5",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "author": {
    "name": "Manuel Fernandez",
    "email": "manferlo81@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manferlo81/rollup-plugin-export-equals.git"
  },
  "bugs": {
    "url": "https://github.com/manferlo81/rollup-plugin-export-equals/issues"
  },
  "homepage": "https://github.com/manferlo81/rollup-plugin-export-equals#readme",
  "license": "MIT"
}
